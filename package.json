{
  "name": "new-mini-app-quickstart",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "create-issues": "node create-github-issues.js",
    "test-issues": "node create-github-issues.js --test",
    "compile": "cd smart-contracts && hardhat compile",
    "test": "cd smart-contracts && hardhat test",
    "test:gas": "cd smart-contracts && REPORT_GAS=true hardhat test",
    "node": "cd smart-contracts && hardhat node",
    "deploy:local": "cd smart-contracts && hardhat run scripts/deploy.ts --network localhost",
    "deploy:baseSepolia": "cd smart-contracts && hardhat run scripts/deploy.ts --network baseSepolia",
    "deploy:base": "cd smart-contracts && hardhat run scripts/deploy.ts --network base",
    "verify": "cd smart-contracts && hardhat verify",
    "clean": "cd smart-contracts && hardhat clean"
  },
  "dependencies": {
    "@coinbase/onchainkit": "latest",
    "@farcaster/miniapp-sdk": "^0.1.8",
    "@tanstack/react-query": "^5.81.5",
    "next": "15.3.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "viem": "^2.31.6",
    "wagmi": "^2.16.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@farcaster/quick-auth": "^0.0.7",
    "@next/eslint-plugin-next": "^15.3.4",
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "@nomicfoundation/hardhat-verify": "^2.1.3",
    "@octokit/rest": "^20.0.0",
    "@openzeppelin/contracts": "^5.4.0",
    "@types/chai": "^5.2.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "hardhat": "^2.27.1",
    "typescript": "^5"
  }
}
